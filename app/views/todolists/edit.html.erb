<%= form_for @todolist do |f| %>

	<%= f.label :title, "Title" %>
	<%= f.text_field :title %>

	<br>
	<br>

	<%= f.label :duedate, "Duedate" %>
	<%= f.text_field :duedate %>

	<br>
	<br>

	<%= f.label :description, "Description" %>
	<%= f.text_field :description %>

	<br>
	<br>

	<%= f.submit "Update" %>

<% end %>